{
  "api": [
    {
      "name": "get_ASCIIArtRender",
      "url": "https://api.getit.ai/openapi.json/render",
      "description": "Renders text as ASCII art. Output is a string that should be rendered a markdown code block. Do not specify a programming language for the code block; it is plain text.",
      "parameters": {
        "properties": {
          "input": {
            "description": "Input text to convert",
            "type": "string",
            "example": "boo"
          },
          "font": {
            "description": "Which ASCII art font to render",
            "type": "string",
            "default": "Ghost",
            "enum": [
              "Standard",
              "Ghost",
              "3D Diagonal",
              "Graffiti",
              "Dancing Font",
              "Big Money-ne",
              "Big Money-nw",
              "Big Money-se",
              "Big Money-sw",
              "Big",
              "Bulbhead",
              "Doom",
              "Isometric1",
              "Slant",
              "Sub-Zero",
              "ANSI Regular",
              "ANSI Shadow",
              "Bloody",
              "Alligator",
              "Alligator2",
              "Ivrit",
              "Larry 3D",
              "NScript",
              "Poison",
              "Small Poison"
            ]
          }
        },
        "required": ["input"],
        "type": "object"
      }
    }
  ],
  "author": "LobeHub",
  "createAt": "2023-08-15",
  "homepage": "https://github.com/lobehub/chat-plugin-ascii-art",
  "identifier": "ascii-art",
  "meta": {
    "avatar": "ðŸŽ¨",
    "tags": ["art", "ascii"],
    "title": "ChatGPT ASCII Art Plugin",
    "description": "Renders text as ASCII art."
  },
